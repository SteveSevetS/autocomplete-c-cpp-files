{
	"name": "autocomplete-c-cpp-files",
	"displayName": "Autocomplete C/C++ files",
	"description": "An extension to become more productive when working with C/C++",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.writeimplfile",
		"onCommand:extension.parsemainfile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.writeimplfile",
				"title": "AUTOCOMPLETE C/C++ FILES: Write Implementation File"
			},
			{
				"command": "extension.parsemainfile",
				"title": "AUTOCOMPLETE C/C++ FILES: Parse Main File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.43.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
